					</div>
				</div>
			</div> -->
			<!-- /banner_2 -->


			<div class="row">
				<div class="col-lg-9">
					<div class="row">
						<div class="col-md-12">
						  <h3></h3>
					
						  <ul class="nav nav-tabs" id="myTab" role="tablist">
							<li class="nav-item">
							<a class="nav-link active" id="home-tab" data-bs-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true" style=" color:rgb(168, 166, 165) ">即時活動資訊</a>
							</li>
							<li class="nav-item">
							<a class="nav-link" id="done-tab" data-bs-toggle="tab" href="#done" role="tab" aria-controls="done" aria-selected="false" style=" color:rgb(168, 166, 165) ">已結束的活動</a>
							</li>
						  </ul>
					    
						  <div class="tab-content" id="myTabContent">
							<!-- tab1 -->
							<div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
								<div class="container margin_60">
									<div class="row add_bottom_60 ">
										<% Events.reverse().forEach(function(Events) { %>
											<div class="col-12 col-md-6 col-sm-12">
												<div class="img_wrapper_gallery">
													<div class="img_container_gallery">
														<a href="/skateboard.map.spot/event/<%= Events.id %>" title="Photo title">
															<% if (Events.picture) { %>
																<img src="<%= Events.picture %>" alt="Image" class="img-fluid" loading="lazy" >
															<% } else { %>
																<img src="map/img/emptyEvent.png" alt="Image" class="img-fluid" loading="lazy" >
															<% } %>
															<i class="icon-resize-full-2"></i>
														</a>
													</div>
												</div>
											</div>
										<% }) %>
									</div>
								</div>
							</div>

							<!-- tab2 -->
							<div class="tab-pane fade" id="done" role="tabpanel" aria-labelledby="done-tab">
								<div class="container margin_60">
										<div class="row add_bottom_60 ">
											<% eventsDone.reverse().forEach(function(eventsDone) { %>
												<div class="col-12 col-md-6 col-sm-12">
													<div class="img_wrapper_gallery">
														<div class="img_container_gallery">
															<a href="/skateboard.map.spot/event/<%= eventsDone.id %>" title="Photo title">
																<% if (eventsDone.picture) { %>
																	<img src="<%= eventsDone.picture %>" alt="Image" class="img-fluid" loading="lazy" >
																<% } else { %>
																	<img src="map/img/emptyEvent.png" alt="Image" class="img-fluid" loading="lazy">
																<% } %>
																<i class="icon-resize-full-2"></i>
															</a>
														</div>
													</div>
												</div>
											<% }) %>
										</div>
								</div>
							</div>
							
							<!-- tab3 -->
							<!-- <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
								<a href="/skateboard.map.spot/coming" class="btn_1 outline">尚未開放</a> 
							</div> -->
						  </div>
						</div><!-- End col-md-12-->
					  </div><!-- End row-->					
					
				</div>
				<!-- End col -->

				<aside class="col-lg-3">

					<div class="widget">
						<div class="input-group">
							<input type="text" class="form-control" placeholder="Search...">
							<span class="input-group-btn">
						<button class="btn btn-default" type="button" style="margin-left:0;"><i class="icon-search"></i></button>
						</span>
						</div>
						<!-- /input-group -->
					</div>
					<!-- End Search -->
					<hr>
					<hr>
					<div class="widget tags">
						<h4>Tags</h4>
						<a href="/skateboard.map.spot/events">技術板Shortboard</a>
						<a href="/skateboard.map.spot/events">LDP</a>
						<a href="/skateboard.map.spot/events">交通板Cruiser</a>
						<a href="/skateboard.map.spot/events">長板Longboard</a>
						<a href="/skateboard.map.spot/events">小魚板Mini Cruiser</a>
						<a href="/skateboard.map.spot/events">衝浪滑板Surfskate</a>
						<a href="/skateboard.map.spot/events">電動滑板Electric</a>
						<a href="/skateboard.map.spot/events">蛇板Snakeboard</a>
					</div>
